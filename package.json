{
  "name": "chrome-extension-rollup-sample",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "clean": "pnpm rmdir dist && pnpm cleandep",
    "cleandep": "pnpm rmdir node_modules",
    "rmdir": "node -e \"var fs = require('fs'); try{process.argv.slice(1).map((fpath) => fs.rmdirSync(fpath, { recursive: true }))}catch(err){console.log(err)}; process.exit(0);\"",
    "build": "rollup --config",
    "start": "rollup --config --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "eslint": "8",
    "eslint-config-airbnb": "^19.0.4",
    "rollup": "^4.30.1",
    "rollup-plugin-copy": "^3.5.0"
  }
}
